#!/bin/bash
echo "alias ll='ls -al'" >> ~/.bashrc
source ~/.bashrc

mv /home/work/github.com/liziwei01/go-liziwei01-library/commit-msg /home/work/github.com/liziwei01/go-liziwei01-library/.git/hooks/
